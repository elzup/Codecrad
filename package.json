{
  "name": "codecrad",
  "version": "1.0.0",
  "description": "Coding game speedrun",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "elzup/codecrad",
  "author": "elzup <guild0105@gmail.com> (elzup.com)",
  "bin": {
    "codecrad": "dist/cli.js"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "cli": "ts-node src/cli.ts",
    "cli-ems": "node --loader ts-node/esm src/cli.ts",
    "build": "tsc && cp -r src/stages dist/ && cp .prettierrc dist",
    "deploy": "yarn run build && np",
    "add-contributor": "all-contributors add",
    "precommit": "lint-staged",
    "lint": "eslint --ext .ts,.tsx src",
    "test": "jest src --detectOpenHandles",
    "test:cov": "jest src --coverage --ci --runInBand",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli-app",
    "cli-game",
    "cli",
    "typing-game",
    "speedrun",
    "game"
  ],
  "dependencies": {
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "diff": "^5.0.0",
    "fs-extra": "^10.0.1",
    "ink": "^3.2.0",
    "meow": "^10.1.1",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^2.0.0",
    "@swc/core": "^1.2.151",
    "@swc/jest": "^0.2.20",
    "@types/chalk": "2.2.0",
    "@types/chokidar": "2.1.3",
    "@types/diff": "5.0.1",
    "@types/fs-extra": "9.0.13",
    "@types/jest": "^27.4.1",
    "@types/meow": "6.0.0",
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "@types/sinon": "10.0.11",
    "all-contributors-cli": "6.20.0",
    "eslint": "8.10.0",
    "eslint-config-nzap": "5.1.0",
    "husky": "^7.0.4",
    "ink-testing-library": "2.1.0",
    "jest": "^27.5.1",
    "lint-staged": "12.3.5",
    "prettier": "2.5.1",
    "sinon": "13.0.1",
    "strip-ansi": "7.0.1",
    "ts-jest": "27.1.3",
    "ts-node": "10.7.0",
    "typescript": "4.6.2"
  }
}
